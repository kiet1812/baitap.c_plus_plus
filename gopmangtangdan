#include <bits/stdc++.h>
using namespace std;
int main(){
    long long n,m; cin>>n>>m;
    long long a1[n],a2[m];
    for(int i=0;i<n;i++){
        cin>>a1[i];
    }
    for(int i=0;i<m;i++){
    	cin>>a1[i];
    }
    long long arr[n+m];
    for(long long i=0;i<n+m;i=i+2){
        arr[i]=a1[i];
        arr[i+1]=a2[i];
    }
    for(long long i=0;i<n+m;i++){
        cout<<arr[i]<<" ";
    }
    return 0;
}
